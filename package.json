{
  "name": "@asleepace/utils",
  "version": "0.1.0",
  "description": "A lightweight collection of JS/TS utils.",
  "module": "src/index.ts",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit",
    "dev": "bun run src/index.ts",
    "lint": "tsc --noEmit && bun test"
  },
  "keywords": [
    "utils",
    "type-safe",
    "helpers"
  ],
  "author": "Colin",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ]
}